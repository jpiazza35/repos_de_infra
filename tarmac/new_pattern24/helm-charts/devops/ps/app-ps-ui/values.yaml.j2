fullnameOverride: {{ project_name }}
namespace:
  name: ps

service:
  port: 443
  targetPort: 443
  name: {{ project_name }}
  type: ClusterIP

image:
  repository: '{{ ss_tools_ecr_url }}/{{ project_name }}-ecr-repo'
  tag: '{{ docker_image_tag }}'
  pullPolicy: 'Always'

replicaCount: 1

autoscaling:
  enabled: false
  minReplicas: 2
  maxReplicas: 100
  targetCPUUtilizationPercentage: 80

ingress:
  enabled: false

environment: devops
