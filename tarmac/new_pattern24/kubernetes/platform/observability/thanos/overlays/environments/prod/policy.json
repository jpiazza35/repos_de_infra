{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "s3Create",
        "Effect": "Allow",
        "Action": [
          "s3:CreateBucket"
          
        ],
        "Resource": [
          "*"
        ]
      },
      {
        "Sid": "s3access",
        "Effect": "Allow",
        "Action": [
          "s3:*"
        ],
        "Resource": [
          "arn:aws:s3:::cluster-prod-prometheus-thanos-bucket"
        ]
      }
    ]
  }