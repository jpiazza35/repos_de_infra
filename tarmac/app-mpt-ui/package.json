{
  "name": "cn-market-pricing-web-ui",
  "private": true,
  "version": "0.1.3",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "staging": "vite --mode staging",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --quiet src --ext .js,.svelte",
    "lint-and-apply": "prettier --write '{public,src}/**/*.{css,html,js,svelte}' && eslint --fix --quiet src --ext .js,.svelte",
    "prepare": "husky install",
    "commit": "cz",
    "cypress": "npx cypress open",
    "build:project": "dotnet clean ../app-mpt-project-service/app-mpt-project-service.sln && dotnet build ../app-mpt-project-service/app-mpt-project-service.sln",
    "build:organization": "dotnet clean ../app-organization-service/app-organization-service.sln && dotnet build ../app-organization-service/app-organization-service.sln",
    "build:survey": "dotnet clean ../app-survey-service/app-survey-service.sln && dotnet build ../app-survey-service/app-survey-service.sln",
    "build:user": "dotnet clean ../app-user-service/app-user-service.sln && dotnet build ../app-user-service/app-user-service.sln",
    "build:incumbent": "dotnet clean ../app-incumbent-service/app-incumbent-service.sln && dotnet build ../app-incumbent-service/app-incumbent-service.sln",
    "build-backend": "npm-run-all --parallel build:*",
    "checkout:db": "git -C ../app-mpt-postgres-db/ checkout main && git -C ../app-mpt-postgres-db/ pull && git -C ../app-mpt-sql-db/ checkout main && git -C ../app-mpt-sql-db/ pull && git -C ../app-incumbent-db/ checkout main && git -C ../app-incumbent-db/ pull",
    "checkout:project": "git -C ../app-mpt-project-service/ checkout main && git -C ../app-mpt-project-service/ pull",
    "checkout:organization": "git -C ../app-organization-service/ checkout main && git -C ../app-organization-service/ pull",
    "checkout:survey": "git -C ../app-survey-service/ checkout main && git -C ../app-survey-service/ pull",
    "checkout:user": "git -C ../app-user-service/ checkout main && git -C ../app-user-service/ pull",
    "checkout:incumbent": "git -C ../app-incumbent-service/ checkout main && git -C ../app-incumbent-service/ pull",
    "checkout:ui": "git checkout main && git pull",
    "checkout-main": "npm-run-all --parallel checkout:*",
    "submodule-update:project": "git -C ../app-mpt-project-service/ submodule update --remote ",
    "submodule-update:organization": "git -C ../app-organization-service/ submodule update --remote",
    "submodule-update:survey": "git -C ../app-survey-service/ submodule update --remote",
    "submodule-update:incumbent": "git -C ../app-incumbent-service/ submodule update --remote",
    "submodule-update:user": "git -C ../app-user-service/ submodule update --remote",
    "submodule-update": "npm-run-all --parallel submodule-update:*",
    "checkout-mock:project": "git -C ../app-mpt-project-service/ checkout 'POC/79933-mock-authentication' && git -C ../app-mpt-project-service/ branch -u 'origin/main' && git -C ../app-mpt-project-service/ pull --no-edit --all",
    "checkout-mock:survey": "git -C ../app-survey-service/ checkout 'POC/79933-mock-authentication' && git -C ../app-survey-service/ branch -u 'origin/main' && git -C ../app-survey-service/ pull --no-edit --all",
    "checkout-mock:user": "git -C ../app-user-service/ checkout 'POC/79933-mock-authentication' && git -C ../app-user-service/ branch -u 'origin/main' && git -C ../app-user-service/ pull --no-edit --all",
    "checkout-mock:incumbent": "git -C ../app-incumbent-service/ checkout 'POC/79933-mock-authentication' && git -C ../app-incumbent-service/ branch -u 'origin/main' && git -C ../app-incumbent-service/ pull --no-edit --all",
    "checkout-mock": "npm-run-all --parallel checkout-mock:*",
    "start:project": "dotnet run --project ../app-mpt-project-service/rest-api",
    "start:organization": "dotnet run --project ../app-organization-service/grpc-server",
    "start:survey-api": "dotnet run --project ../app-survey-service/rest-api",
    "start:survey-server": "dotnet run --project ../app-survey-service/grpc-server",
    "start:user-api": "dotnet run --project ../app-user-service/rest-api",
    "start:user-server": "dotnet run --project ../app-user-service/grpc-server",
    "start:incumbent-api": "dotnet run --project ../app-incumbent-service/rest-api",
    "start:incumbent-server": "dotnet run --project ../app-incumbent-service/grpc-server",
    "start-backend": "npm-run-all --parallel start:*",
    "start-sql": "docker run -d -it -p 1433:1433 sqldb:latest",
    "start-mpt-db": "docker run -d -it -p 5433:5432 sqlmptdb:latest",
    "start-incumbent-db": "docker run -d -it -p 5434:5432 sqlincumbentdb:latest",
    "start-incumbent-staging-db": "docker run -d -it -p 5435:5432 sqlincumbentstagingdb:latest",
    "start-db": "npm run start-sql & npm run start-mpt-db & npm run start-incumbent-db & npm run start-incumbent-staging-db",
    "stop-db": "docker kill $(docker ps -q)",
    "remove-db-images": "docker rmi -f $(docker images -a -q)",
    "build-db": "docker build -t sqldb:latest ../app-mpt-sql-db && docker build -t sqlmptdb:latest ../app-mpt-postgres-db && docker build -t sqlincumbentdb:latest ../app-incumbent-db && docker build -t sqlincumbentstagingdb:latest ../app-incumbent-staging-db",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.1.0",
    "@types/dompurify": "^3.0.5",
    "cypress": "^12.8.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jquery": "^1.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte": "^2.28.0",
    "gridjs-svelte": "latest",
    "https": "^1.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "prettier-plugin-svelte": "^2.9.0",
    "simple-svelte-autocomplete": "^2.5.0",
    "standard-version": "^9.5.0",
    "svelte": "^3.59.1",
    "vite": "^3.2.4"
  },
  "dependencies": {
    "@amcharts/amcharts5": "^5.5.1",
    "@auth0/auth0-spa-js": "^2.1.2",
    "@azure/msal-browser": "^2.32.1",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@sveltestack/svelte-query": "^1.6.0",
    "axios": "^1.2.1",
    "crypto-js": "^4.1.1",
    "dompurify": "^3.0.8",
    "exceljs": "^4.3.0",
    "jquery": "^3.6.1",
    "launchdarkly-js-client-sdk": "^3.1.4",
    "lodash.intersection": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.zip": "^4.2.0",
    "moment": "^2.29.4",
    "object-to-formdata": "^4.4.2",
    "svelte-forms-lib": "^2.0.1",
    "svelte-routing": "^1.6.0",
    "sveltestrap": "^5.10.0",
    "sweetalert2": "^11.4.8",
    "yup": "^0.32.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,svelte}": [
      "eslint",
      "prettier --write"
    ]
  }
}
